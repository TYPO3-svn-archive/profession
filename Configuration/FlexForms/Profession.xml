<T3DataStructure>
	<meta type="array">
		<langChildren type="integer">0</langChildren>
		<langDisable type="integer">1</langDisable>
	</meta>
	<ROOT>
		<type>array</type>
		<el>
			<settings.listLimit>
				<TCEforms>
					<label>listLimit</label>
					<config>
						<type>input</type>
						<size>10</size>
						<default>5</default>
					</config>
				</TCEforms>
			</settings.listLimit>
			<settings.emailAddress>
				<TCEforms>
					<label>Application-Address</label>
					<config>
						<type>input</type>
						<size>30</size>
						<eval>required</eval>
					</config>
				</TCEforms>
			</settings.emailAddress>
			<settings.candidateType>
				<TCEforms>
					<label>Select Candidate Types</label>
					<config>
						<type>select</type>
						<foreign_table>tx_profession_domain_model_candidatetype</foreign_table>
						<minitems>1</minitems>
						<maxitems>100</maxitems>
						<size>5</size>
					</config>
				</TCEforms>
			</settings.candidateType>
		</el>
	</ROOT>
</T3DataStructure>


